{
  "name": "mernreduxboilerplate",
  "version": "1.0.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "webpack -p --progress --profile --colors && node server",
    "start:dev": "node server",
    "test": "jest --env=jsdom",
    "test:watch": "jest --watch --verbose"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "<rootDir>/client/app/setupTests.js",
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.svg$": "<rootDir>/client/app/svgTransform.js"
    },
    "moduleNameMapper": {
      ".+\\.(css|styl|less|sass|scss|png|jpg|ttf|woff|woff2)$": "identity-obj-proxy"
    },
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/client/app/components/Arithmetic/"
    ],
    "unmockedModulePathPatterns": [
      "react",
      "react-dom",
      "react-native",
      "react-addons-test-utils",
      "promise",
      "source-map",
      "enzyme"
    ]
  },
  "engines": {
    "node": ">=6"
  },
  "dependencies": {
    "@material-ui/core": "^1.2.2",
    "autoprefixer": "^8.6.2",
    "axios": "^0.18.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "bcrypt": "^3.0.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "connect-history-api-fallback": "^1.5.0",
    "copy-webpack-plugin": "^4.5.1",
    "css-loader": "^0.28.11",
    "express": "^4.16.3",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "html-webpack-plugin": "^3.1.0",
    "jquery": "^3.3.1",
    "json-stringify": "^1.0.0",
    "jsonwebtoken": "^8.3.0",
    "lodash": "^4.17.10",
    "materialize-css": "^1.0.0-rc.2",
    "mongojs": "^2.6.0",
    "mongoose": "^5.2.9",
    "node-sass": "^4.9.0",
    "nodemon": "^1.17.2",
    "npm": "^6.3.0",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "postcss-loader": "^2.1.3",
    "prop-types": "^15.6.2",
    "react": "^16.2.0",
    "react-circular-progressbar": "^1.0.0",
    "react-dom": "^16.2.0",
    "react-draggable": "^3.0.5",
    "react-hot-loader": "^4.3.2",
    "react-materialize": "^2.3.1",
    "react-parallax": "^1.8.0",
    "react-redux": "^5.0.7",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.2.2",
    "react-scripts": "^1.1.4",
    "react-svg": "^4.0.13",
    "react-toastify": "^4.1.0",
    "recompose": "^0.27.1",
    "redux": "^4.0.0",
    "redux-devtools-extension": "^2.13.2",
    "redux-thunk": "^2.3.0",
    "sass-loader": "^7.0.3",
    "style-loader": "^0.21.0",
    "validator": "^10.4.0",
    "webpack": "^4.12.0",
    "webpack-cli": "^3.0.4",
    "webpack-dev-middleware": "^3.0.1",
    "webpack-hot-middleware": "^2.21.2",
    "webpack-merge": "^4.1.2",
    "whatwg-fetch": "^2.0.4"
  },
  "devDependencies": {
