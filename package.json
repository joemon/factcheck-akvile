    "file-loader": "^1.1.11",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^23.1.0",
    "react-addons-test-utils": "^15.6.2",
    "react-test-renderer": "^16.4.0",
    "svg-inline-loader": "^0.8.0",
    "uglify-js-brunch": "^2.10.0",
    "url-loader": "^1.0.1"
  }
}
